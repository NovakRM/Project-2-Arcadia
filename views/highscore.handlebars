<div class="container">
  <div class="header">
    <h2 class="hTitle">Arcadia</h2>
    <div class="hNav">
      <a href="/"><i class="fa fa-home fa-lg" aria-hidden="true" alt="home icon"></i></a>
      <a href="/profile"><i class="fa fa-user fa-lg" aria-hidden="true" alt="user icon, check profile"></i></a>
      <a href="/game"><i class="fa fa-gamepad fa-lg" aria-hidden="true" alt="game icon, play a game"></i></a>
      <a href="/highscore"><i class="fa fa-trophy fa-lg" aria-hidden="true" alt="trophy icon, check high scores"></i></a>
    </div>
  </div>

   <div class="modal" style="display:block">
      <div class="scores">
        <h2 class="welcomeUser">Grogu Quest</h2>
      </div>
      <div class="dashBody">
        <div class="options">
            {{#each scores}}
              {{> scores/machOneScore-block }}
            {{/each}}
        </div>
      </div>
  </div>

  <div class="footer">
      <div class="fNav">
        <a href="/"><i class="fa fa-home fa-lg" aria-hidden="true" alt="home icon"></i></a>
        <a href="/profile"><i class="fa fa-user fa-lg" aria-hidden="true" alt="user icon, check profile"></i></a>
        <a href="/grogu-quest"><i class="fa fa-gamepad fa-lg" aria-hidden="true" alt="game icon, play a game"></i></a>
        <a href="/highscore"><i class="fa fa-trophy fa-lg" aria-hidden="true" alt="trophy icon, check high scores"></i></a>
    </div>
  </div>
</div>
